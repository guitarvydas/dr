#!/usr/bin/env node
const fs = require ('fs');
var rawText = fs.readFileSync ('/dev/fd/0');
var parameters = JSON.parse(rawText);
parameters.forEach (p => {
var Passed = p [0];
var Edge = p [1];
if (!p.Passed) {
  console.error ('design rule failure');
console.error(``);
console.error(`Edge Containment edge=${Edge}`);
console.error(``);
console.error(``);
console.error(`	`);

}
});

